@{
    ViewBag.Title = "Om os";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="login">
    <h1>Login to Web App</h1>
    <form method="post" action="Index">
        <p><input type="text" name="login" value="" placeholder="Studienummer"></p>
        <p><input type="password" name="password" value="" placeholder="Adgangskode"></p>
        <p class="remember_me">
            <label>
                <input type="checkbox" name="remember_me" id="remember_me">
                Husk mig på denne computer
            </label>
        </p>
        <p class="submit"><input type="submit" name="commit" value="Login"></p>
    </form>
</div>
